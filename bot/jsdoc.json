{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [
            "commands",
            "events",
            "handlers",
            "services",
            "settings",
            "templates"
        ],
        "includePattern": ".+\\.js(docs|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "destination": "./docs",
        "template": "node_modules/docdash",
        "recurse": true
    }
}