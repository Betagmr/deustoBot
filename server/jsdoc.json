{
    "source": {
      "include": [
        "models",
        "utils"
      ],
      "includePattern": ".js$",
      "excludePattern": "(node_modules|docs)"
    },
    "plugins": [],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "destination": "./docs",
      "template": "node_modules/docdash"
    }
  }